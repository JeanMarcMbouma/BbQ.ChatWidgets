{
  "metadata": [
    {
      "src": [
        { "files": ["BbQ.ChatWidgets/bin/Release/net8.0/BbQ.ChatWidgets.dll", "BbQ.ChatWidgets/bin/Release/net8.0/BbQ.ChatWidgets.xml"] }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/**.md",
          "README.md",
          "api/**.yml"
        ],
        "exclude": [
          "**/node_modules/**",
          "js/**",
          "Sample/**/node_modules/**",
          "**/ClientApp/**/node_modules/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.*",
          "!**/*.csproj",
          "!**/*.dll",
          "!**/*.xml"
          ,
          "!**/node_modules/**",
          "!js/node_modules/**",
          "!**/ClientApp/**/node_modules/**",
          "!Sample/**/node_modules/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "BbQ.ChatWidgets",
      "_disableContribution": true
    },
    "template": [
      "default"
    ]

  }
}
