{
  "build": {
    "content": [
      {
        "files": [
          "docs/**.md",
          "README.md",
          "api/**.yml"
        ],
        "exclude": [
          "**/node_modules/**",
          "js/**",
          "Sample/**/node_modules/**",
          "**/ClientApp/**/node_modules/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/**",
          "styles/**",
          "favicon.ico",
          "logo.svg",
          "manifest.json",
          "search-stopwords.json"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "BbQ.ChatWidgets",
      "_disableContribution": true
    },
    "template": [
      "default"
    ]

  }
}
