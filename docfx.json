{
  "metadata": [
    {
      "src": [
        { "files": ["BbQ.ChatWidgets/BbQ.ChatWidgets.csproj"] }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
