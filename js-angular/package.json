{
  "name": "@bbq-chat/widgets-angular",
  "version": "1.0.0",
  "description": "Angular components and services for BbQ ChatWidgets",
  "license": "MIT",
  "author": "BbQ ChatWidgets Contributors",
  "homepage": "https://github.com/JeanMarcMbouma/BbQ.ChatWidgets",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeanMarcMbouma/BbQ.ChatWidgets.git",
    "directory": "js-angular"
  },
  "bugs": {
    "url": "https://github.com/JeanMarcMbouma/BbQ.ChatWidgets/issues"
  },
  "keywords": [
    "angular",
    "chat",
    "widgets",
    "ui",
    "components",
    "bbq-chat"
  ],
  "main": "./dist/fesm2022/bbq-chat-widgets-angular.mjs",
  "module": "./dist/fesm2022/bbq-chat-widgets-angular.mjs",
  "esm2022": "./dist/esm2022/bbq-chat-widgets-angular.mjs",
  "fesm2022": "./dist/fesm2022/bbq-chat-widgets-angular.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "esm2022": "./dist/esm2022/bbq-chat-widgets-angular.mjs",
      "esm": "./dist/esm2022/bbq-chat-widgets-angular.mjs",
      "default": "./dist/fesm2022/bbq-chat-widgets-angular.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "test": "echo \"No tests yet\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "peerDependencies": {
    "@angular/common": ">=17.0.0",
    "@angular/core": ">=17.0.0",
    "@bbq-chat/widgets": "^1.0.0"
  },
  "devDependencies": {
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@bbq-chat/widgets": "file:../js",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "ng-packagr": "^17.3.0",
    "prettier": "^3.1.1",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.4.2",
    "zone.js": "^0.14.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
